{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::observatorio-inmobiliario",
      "Condition": {
        "StringLike": {
          "s3:prefix": ["models/*"]
        }
      }
    },
    {
      "Effect": "Allow",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3::observatorio-inmobiliario/models/*"
    }
  ]
}
